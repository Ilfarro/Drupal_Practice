{#
/**
 * @file challenge.html.twig
 * Default theme implementation to present Challenge data.
 *
 * This template is used when viewing Challenge pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_challenge()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('challenge') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
