{#
/**
 * @file live_code.html.twig
 * Default theme implementation to present Live code data.
 *
 * This template is used when viewing Live code pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_live_code()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('live_code') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
