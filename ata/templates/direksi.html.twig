{#
/**
 * @file direksi.html.twig
 * Default theme implementation to present Direksi data.
 *
 * This template is used when viewing Direksi pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_direksi()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('direksi') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
